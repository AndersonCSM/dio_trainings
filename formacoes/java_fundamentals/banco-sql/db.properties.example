# Exemplo de configuração do banco de dados
# Copie este arquivo para db.properties e configure com suas credenciais reais
# IMPORTANTE: O arquivo db.properties está no .gitignore e não será versionado

# Configurações de Conexão
db.host=localhost
db.port=3306
db.name=banco_sql
db.url=jdbc:mysql://${db.host}:${db.port}/${db.name}

# Credenciais (NUNCA commite senhas reais!)
db.username=seu_usuario
db.password=sua_senha

# Configurações Adicionais
db.driver=com.mysql.cj.jdbc.Driver
db.pool.size=10
db.connection.timeout=30000
